    "class SearchPageMixIn():\n",
    "\n",
    "    lastdate = LastDate()\n",
    "\n",
    "    def _county_specific_selenium_search(self):\n",
    "        raise CountyNotSupported('County is not supported. Are you a developer? Did you forget to provide county-specific steps for searching?')\n",
